<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="custom_base" name="Custom G2P Registration Portal Base" inherit_id="g2p_portal_base.base">
        <xpath expr="//header//img[@class='main-logo']" position="attributes">
            <attribute
                name="src"
            >/g2p_service_provider_portal_reimbursement/static/src/img/logo@2x.png</attribute>
        </xpath>

        <xpath expr="//footer//img[@class='main-logo']" position="attributes">
            <attribute
                name="src"
            >/g2p_service_provider_portal_reimbursement/static/src/img/logo@2x.png</attribute>
        </xpath>
    </template>
</odoo>
