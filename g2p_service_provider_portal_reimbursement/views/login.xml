<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="service_provider_login_page"
        name="G2P Service Provider Portal Login"
        inherit_id="g2p_portal_base.login_page"
    >
        <xpath expr="//div[@class='logo-box']/img" position="attributes">
            <attribute
                name="src"
            >/g2p_service_provider_portal_reimbursement/static/src/img/logo@2x.png</attribute>
        </xpath>
        <xpath expr="//t[@t-call='g2p_portal_base.base']" position="inside">
            <t t-set="title">service provider Portal Login</t>
        </xpath>
    </template>
</odoo>
